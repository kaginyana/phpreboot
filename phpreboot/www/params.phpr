  <html>
   <title>PHP.reboot Parameters</title>
   <body>
   
    <h3>_SERVER</h3>
    <table>
     <tr>
       <td>Key</td><td>Value</td>
     </tr>
     {
       foreach(_SERVER as key: value) {
         <tr>
           <td>$key</td><td>$value</td>
         </tr>
       }
     }
    </table>
    
    <h3>_GET</h3>
    <table>
     <tr>
       <td>Key</td><td>Value</td>
     </tr>
     {
       foreach(_GET as key: value) {
         <tr>
           <td>$key</td><td>$value</td>
         </tr>
       }
     }
    </table>
    
    <h3>_POST</h3>
    <table>
     <tr>
       <td>Key</td><td>Value</td>
     </tr>
     {
       foreach(_POST as key: value) {
         <tr>
           <td>$key</td><td>$value</td>
         </tr>
       }
     }
    </table>
    
   </body>
  </html>
