
function cons(element, next) {
  return {"element": element, "next": next}
}

function nil() {
  return null
}

function size(list) {
  if (list == null)
    return 0
  return 1 + size(list.next)
}

function contains(list, element) {
  for(l = list; l != null; l = l.next) {
    if (l.element == element)
      return true
  }
  return false
}

function display(list) {
  for(l = list; l != null; l = l.next) {
    echo l.element
  }
  return null
}

